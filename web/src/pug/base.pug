//- module
doctype html
html
  block vars
  include @/@loadingio/bootstrap.ext/index.pug
  head
    block prehead
    +css([
      {name: "bootstrap", path: "dist/css/bootstrap.min.css"},
      {name: "@loadingio/bootstrap.ext"},
      {name: "ldiconfont"},
      {name: "ldcover"},
      {name: "ldbutton"},
      {name: "ldloader"},
      {name: "@loadingio/loading.css", path: "lite.min.css"},
      {url: "/css/index.css"}
    ], {pack: false})
    block head
  body

    block body
    +script([
      {name: "proxise"},
      {name: "@loadingio/ldquery"},
      {name: "@plotdb/json0", path: "json0.min.js"},
      {name: "bootstrap.native", path: "dist/bootstrap-native-v4.min.js"},
      {name: "@loadingio/debounce.js"},
      {name: "ldcover"},
      {name: "ldloader"},
      {name: "lderror"},
      {name: "ldview"},
      {name: "@plotdb/semver"},
      {name: "@plotdb/datadom"},
      {name: "@plotdb/csscope"},
      {name: "@plotdb/rescope"},
      {name: "@plotdb/block", version: "dev"}
    ], {pack: false})
    block script
