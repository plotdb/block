div
  script: :lsc
    do
      pkg:
        dependencies: [
          {url: "https://unpkg.com/react@17/umd/react.development.js", async: false},
          "https://unpkg.com/react-dom@17/umd/react-dom.development.js"
        ],
      init: ({root, context}) ->
        Hello = (props) -> return React.createElement('div', null, "React: Hello #{props.name}")
        ReactDOM.render(
          React.createElement(Hello, {name: "world"}, null),
          root
        )
       
